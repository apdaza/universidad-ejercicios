<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Inicio</servlet-name>
        <servlet-class>servlets.Inicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertarContacto</servlet-name>
        <servlet-class>servlets.InsertarContacto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargarContacto</servlet-name>
        <servlet-class>servlets.CargarContacto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarContacto</servlet-name>
        <servlet-class>servlets.ActualizarContacto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Inicio</servlet-name>
        <url-pattern>/Inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertarContacto</servlet-name>
        <url-pattern>/InsertarContacto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargarContacto</servlet-name>
        <url-pattern>/CargarContacto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarContacto</servlet-name>
        <url-pattern>/ActualizarContacto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Inicio</welcome-file>
    </welcome-file-list>
</web-app>
